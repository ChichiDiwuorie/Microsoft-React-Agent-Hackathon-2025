# Environment
NODE_ENV=development
PORT=3000

# API Endpoints
GRAPH_API_ENDPOINT=https://graph.microsoft.com/v1.0
POWERBI_API_ENDPOINT=https://api.powerbi.com/v1.0/myorg

# Logging
LOG_LEVEL=info 